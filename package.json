{
	"name": "@turbowarp/extensions",
	"version": "0.0.1",
	"description": "Unsandboxed extensions for TurboWarp",
	"exports": {
		"./builder": "./development/builder.js"
	},
	"scripts": {
		"start": "node development/server.js --server-type:node",
		"dev": "nodemon development/server.js --server-type:nodemon",
		"build": "node development/build-production.js",
		"validate": "node development/validate.js",
		"lint": "eslint development --max-warnings=0",
		"format": "prettier . --write",
		"check-format": "prettier . --check",
		"upload-translations": "node development/upload-translations.js",
		"download-translations": "node development/download-translations.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TurboWarp/extensions.git"
