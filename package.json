		"markdown-it": "^14.1.0"
	},
	"devDependencies": {
		"@eslint/json": "^0.13.2",
		"@transifex/api": "^7.1.4",
		"espree": "^9.6.1",
		"esquery": "^1.6.0",
		"nodemon": "^3.1.10",
		"prettier": "^3.8.1",
		"spdx-expression-parse": "^4.0.0"
	},
